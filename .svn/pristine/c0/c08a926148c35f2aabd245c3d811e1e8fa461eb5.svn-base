	LODOP.ADD_PRINT_LINE(92,14,91,760,0,1);
	LODOP.ADD_PRINT_LINE(124,14,123,760,0,1);
	LODOP.ADD_PRINT_LINE(156,14,155,760,0,1);
	LODOP.ADD_PRINT_LINE(188,14,187,760,0,1);
